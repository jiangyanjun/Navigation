        function clickrate(data) {
            LoadCliceInfo();
            $.post("/Ajax/Addclickrate", { UrlId: data, UserAgents: navigator.userAgent, Ip: Kebue_ip_address.Ip, Msg: JSON.stringify(Kebue_ip_address) }, function (result) {
            });
        }
        function GeturlDaras(i) {
            $.post("/Ajax/GeturlDaras", { index: i }, function (data) {
                //CallBind(JSON.parse(data));
                CallBind(data);
            });
        }
        function CallBind(data) {
            $("#ajaxloaddata").append(data.msg);
            if (data.nex != 0 || data.nex != "0") {
                GeturlDaras(data.nex);
            } else {
                LoadCliceInfo();
            }
        }
        var Kebue_ip_address;
        function LoadCliceInfo() {
            if (Kebue_ip_address == null || Kebue_ip_address == undefined || Kebue_ip_address == "" || Kebue_ip_address.length < 1) {
                var url = 'http://chaxun.1616.net/s.php?type=ip&output=json&callback=?&_=' + Math.random();
                $.getJSON(url, function (data) {
                    Kebue_ip_address = data;
                    return;
                })
            }
        };
        $(document).ready(function () {
            GeturlDaras(0);
        })
