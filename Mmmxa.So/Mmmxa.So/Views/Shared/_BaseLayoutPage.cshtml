<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <link rel="shortcut icon" href="http://www.kebue.com//images/kebue.ico">
    <link rel="icon" href="http://www.kebue.com//images/kebue.ico">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="shortcut icon" href="http://www.kebue.com//images/kebue.ico">
    <link rel="icon" href="http://www.kebue.com//images/kebue.ico">
    <meta name="renderer" content="webkit" />
    <meta name="revised" content="Chong, 2015/10/11/" />
    <meta name="author" content="kebue.com" />
    <title>@ViewBag.Title</title>
    <meta name="keywords" content=@ViewBag.Keywords />
    <meta name="description" content=@ViewBag.Description />
    <link rel="shortcut icon" href="http://www.kebue.com//images/kebue.ico" />
    <script type="text/javascript" src="http://lib.sinaapp.com/js/jquery/3.1.0/jquery-3.1.0.min.js"></script>
    <script src="http://lib.sinaapp.com/js/jquery-ui/1.10.2/jquery-ui.min.js"></script>
    <link rel="stylesheet" href="http://7q5dsg.com1.z0.glb.clouddn.com/search/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="http://7q5dsg.com1.z0.glb.clouddn.com//css/jquery.mCustomScrollbar.css" />
    <link href="~/Content/css/Search.css" type="text/css" rel="stylesheet" />
    <link href="~/Content/css/data.css" rel="stylesheet" />
    <link href="~/Content/css/img.css" rel="stylesheet" />
    <link href="~/Content/css/soo.css" rel="stylesheet" />
    <script src="~/Content/js/config.js" type="text/javascript" charset="utf-8"></script>
</head>
<body>
    <div>
        @Html.Partial("~/Views/PartialPage/Layout/_PartialPageHeader.cshtml");
        <div class="container">
            <div class="left-nav">
                @RenderBody()
                <script type="text/javascript">
                    var c = localStorage.content;
                    if (c) {
                        $("#foo")[0].innerHTML = c;
                    }
                </script>
                <script src="~/Content/js/jquery.mCustomScrollbar.concat.min.js"></script>
                <script>$(".left-nav").mCustomScrollbar({ theme: "minimal-dark" });</script>
            </div>
            @Html.Partial("~/Views/PartialPage/Layout/_PartialPageContent.cshtml");
        </div>
        @Html.Partial("~/Views/PartialPage/Layout/_PartialPageFoot.cshtml");
    </div>
</body>
</html>
